{
  "name": "syntacticus.org",
  "version": "1.0.0",
  "private": true,
  "description": "The syntacticus.org web site",
  "author": "Marius L. JÃ¸hndal <mariuslj@ifi.uio.no>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && cp dist/index.html dist/200.html",
    "lint": "vue-cli-service lint",
    "deploy": "surge dist",
    "start": "npm run dev",
    "stats": "du dist/static/css/*.css dist/static/js/*.js dist/index.html | cut -f1 | awk '{s+=$1} END {print s}'"
  },
  "dependencies": {
    "@sentry/browser": "5.17.0",
    "@sentry/integrations": "5.17.0",
    "accounting": "0.4.1",
    "axios": "0.21.1",
    "buefy": "0.8.20",
    "core-js": "^3.6.5",
    "d3-array": "1.2.4",
    "d3-axis": "1.0.12",
    "d3-scale": "1.0.7",
    "d3-selection": "1.2.0",
    "d3-shape": "1.2.3",
    "d3-zoom": "1.7.4",
    "font-awesome": "4.7.0",
    "lodash": "4.17.20",
    "vue": "2.6.11",
    "vue-infinite-scroll": "2.0.2",
    "vue-multiselect": "2.0.6",
    "vue-router": "3.0.1",
    "vue-shortkey": "3.1.7",
    "vue-ua": "1.4.0",
    "vuex": "2.3.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "4.4.4",
    "@vue/cli-plugin-router": "4.4.4",
    "@vue/cli-plugin-vuex": "4.4.4",
    "@vue/cli-service": "4.4.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": 0,
      "no-prototype-builtins": 0,
      "vue/require-v-for-key": 0,
      "vue/no-use-v-if-with-v-for": 0,
      "vue/valid-v-for": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "not dead"
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}
