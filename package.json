{
  "name": "syntacticus.org",
  "version": "1.0.0",
  "private": true,
  "description": "The syntacticus.org web site",
  "author": "Marius L. JÃ¸hndal <mariuslj@ifi.uio.no>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && cp dist/index.html dist/200.html",
    "lint": "vue-cli-service lint",
    "deploy": "surge dist",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@sentry/vue": "^6.19.2",
    "accounting": "0.4.1",
    "axios": "^0.26.0",
    "buefy": "^0.9.19",
    "core-js": "^3.6.5",
    "d3-array": "1.2.4",
    "d3-axis": "1.0.12",
    "d3-scale": "1.0.7",
    "d3-selection": "1.2.0",
    "d3-shape": "1.2.3",
    "d3-zoom": "1.7.4",
    "lodash": "4.17.21",
    "vue": "2.6.14",
    "vue-infinite-scroll": "2.0.2",
    "vue-router": "3.0.1",
    "vue-shortkey": "3.1.7",
    "vue-ua": "1.4.0",
    "vuex": "2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-router": "~5.0.4",
    "@vue/cli-plugin-unit-jest": "~5.0.4",
    "@vue/cli-plugin-vuex": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/test-utils": "^1.0.3",
    "@vue/vue2-jest": "^27.0.0-alpha.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.10.0",
    "jest": "^27.1.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "plugin:you-dont-need-lodash-underscore/compatible"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": 0,
      "no-prototype-builtins": 0,
      "vue/multi-word-component-names": 0,
      "vue/no-use-v-if-with-v-for": 0,
      "vue/require-v-for-key": 0,
      "vue/valid-v-for": 0,
      "you-dont-need-lodash-underscore/keys": 0,
      "you-dont-need-lodash-underscore/some": 0,
      "you-dont-need-lodash-underscore/map": 0,
      "you-dont-need-lodash-underscore/reduce": 0,
      "you-dont-need-lodash-underscore/each": 0,
      "you-dont-need-lodash-underscore/filter": 0
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}
